&#5176;canvas id=&#10078;canvas&#10078;&#5171;&#5176;/canvas&#5171;

&#5176;script&#5171;

function plotFunction() {
  var canvas = document.getElementById(&#10078;canvas&#10078;);
  var ctx = canvas.getContext(&#10078;2d&#10078;);

  var xValues = [];
  var yValues = [];

  for (var i = 0; i &#5176;= 100; i++) {
    xValues.push(i);
    yValues.push(i / (100 - i));
  }

  ctx.beginPath();
  
//  ctx.moveTo(0, 0);

startx=0.0;
starty=0.0;

ctx.moveTo( startx, starty );
  
  
  for (var i = 0; i &#5176;= 100; i++) {
//    ctx.lineTo(xValues[i], yValues[i]);
    
    ctx.lineTo( startx + xValues[i], starty + yValues[i]);
    
  }

  ctx.stroke();
}

window.onload = plotFunction;

&#5176;/script&#5171;





