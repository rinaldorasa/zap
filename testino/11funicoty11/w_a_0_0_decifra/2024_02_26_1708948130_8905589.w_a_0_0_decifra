function findOnPage(text) {
  // Cerca tutti gli elementi che contengono la stringa specificata
  const elements = document.querySelectorAll(`*[textContent*=&#10078;${text}&#10078;]`);

  // Evidenzia gli elementi trovati
  for (const element of elements) {
    element.classList.add(&#10078;highlight&#10078;);
  }

  // Scorri alla prima occorrenza
  if (elements.length &#5171; 0) {
    elements[0].scrollIntoView({ behavior: &#10078;smooth&#10078; });
  } else {
    alert(&#10078;Nessuna occorrenza trovata&#10078;);
  }
}

// Esegui la funzione quando l&#10076;utente digita nella casella di testo
const searchInput = document.getElementById(&#10078;search-input&#10078;);
searchInput.addEventListener(&#10078;input&#10078;, () =&#5171; {
  const text = searchInput.value;
  findOnPage(text);
});





