function reload() {
  // Recupera il percorso completo della pagina in corso
  const fullPath = document.location.protocol + &#10078;//&#10078; + document.location.hostname + document.location.pathname;

  // Reload la pagina
  window.location.href = fullPath;
}

&#5176;/script&#5171;

&#5176;/body&#5171;
&#5176;/html&#5171;


  

