    header(&#10076;Location: home.php&#10076;);
    exit;
}

// Controllo se sono stati inviati i dati di login
if (isset($_POST[&#10076;username&#10076;]) && isset($_POST[&#10076;password&#10076;])) {

    // Validazione input
    $username = filter_input(INPUT_POST, &#10076;username&#10076;, FILTER_SANITIZE_STRING);
    $password = filter_input(INPUT_POST, &#10076;password&#10076;, FILTER_SANITIZE_STRING);

    // Controllo se l&#10076;username esiste
    $stmt = $db-&#5171;prepare(&#10076;SELECT * FROM users WHERE username = :username&#10076;);
    $stmt-&#5171;bindValue(&#10076;:username&#10076;, $username);
    $stmt-&#5171;execute();
    $user = $stmt-&#5171;fetch();

    // Se l&#10076;username esiste
    if ($user) {
        // Verifica password
        if (password_verify($password, $user[&#10076;password&#10076;])) {
            // Autenticazione riuscita
            $_SESSION[&#10076;user_id&#10076;] = $user[&#10076;id&#10076;];
            header(&#10076;Location: home.php&#10076;);
            exit;
        } else {
            // Password errata
            echo &#10076;&#5176;p class=&#10078;error&#10078;&#5171;Password errata.&#5176;/p&#5171;&#10076;;
        }
    } else {
        // Username non trovato
        echo &#10076;&#5176;p class=&#10078;error&#10078;&#5171;Username non trovato.&#5176;/p&#5171;&#10076;;
    }
}

