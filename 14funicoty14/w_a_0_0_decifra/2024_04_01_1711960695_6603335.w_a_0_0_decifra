&#5176;button  onclick=&#10078;RodoAttivo(&#10076;SLIDER&#10076;,&#10076;VALORE&#10076;)&#10078; class=&#10078;ui green button&#10078;&#5171;ok&#5176;/button&#5171;
&#5176;button  onclick=&#10078;RodoUscita(&#10076;SLIDER&#10076;,&#10076;VALORE&#10076;)&#10078; class=&#10078;ui red button&#10078;&#5171;ko&#5176;/button&#5171;

&#5176;!-- --&#5171;
&#5176;div id=&#10078;SLIDER&#10078;&#5171;&#5176;/div&#5171;
&#5176;div id=&#10078;VALORE&#10078;&#5171;&#5176;/div&#5171;





&#5176;script&#5171;
function RodoAttivo(slider, valore){



document.getElementById(slider).innerHTML=`&#5176;input id=&#10078;VALORE_INPUT&#10078; type=&#10078;range&#10078; step=&#10078;1&#10078; min=&#10078;0&#10078; max=&#10078;10&#10078; value=&#10078;2.5&#10078;&#5171;`;

document.querySelector(&#10078;#valore&#10078;).style.visibility=&#10078;visible&#10078;;

    function LeggiSlider(){
    ValoreSlider=document.getElementById(&#10078;VALORE_INPUT&#10078;).value;
    document.getElementById(valore).innerHTML=ValoreSlider;

    }; // funzione LeggiSlider
    AttivaLeggiSlider=setInterval(LeggiSlider, 100);

}; // funzione RodoAttivo



function RodoUscita(slider, valore){
document.querySelector(&#10078;#VALORE_INPUT&#10078;).style.visibility=&#10078;hidden&#10078;;
document.querySelector(&#10078;#valore&#10078;).style.visibility=&#10078;hidden&#10078;;

clearInterval(AttivaLeggiSlider);

    function LeggiSlider(){
    ValoreSlider=document.getElementById(&#10078;VALORE_INPUT&#10078;).value;
    document.getElementById(valore).innerHTML=ValoreSlider;

    }; // funzione LeggiSlider

}; // funzione RodoUscita



&#5176;/script&#5171;




 
