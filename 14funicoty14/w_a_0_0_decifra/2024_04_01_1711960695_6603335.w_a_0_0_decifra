&#5176;script&#5171;
function Rodo(slider, valore, settaggi){

document.getElementById(slider).innerHTML=`&#5176;style&#5171;#VALORE_INPUT{position:fixed; top:0.50rem; right:0.50rem;}&#5176;/style&#5171;&#5176;input id=&#10078;VALORE_INPUT&#10078; type=&#10078;range&#10078; step=&#10078;1&#10078; min=&#10078;0&#10078; max=&#10078;10&#10078; value=&#10078;2.5&#10078;&#5171;`;


    function LeggiSlider(){
	
	ValoreSlider=document.getElementById(&#10078;VALORE_INPUT&#10078;).value;
	//  document.getElementById(valore).innerHTML=ValoreSlider;
	
		document.getElementById(settaggi).innerHTML=
		`
		&#5176;style&#5171;
		#IMAGE_1_000{width:`+ValoreSlider*10+`%;}
	
		#IMAGE_1_0000, #IMAGE_1_0001, #IMAGE_1_0002{width:`+ValoreSlider*10+`%;}
	
		&#5176;/style&#5171;
	`;
    }; // funzione LeggiSlider

	AttivaLeggiSlider=setInterval(LeggiSlider, 100);


}; // funzione Rodo

Rodo(&#10078;SLIDER&#10078;,&#10078;VALORE&#10078;,&#10078;SETTAGGI&#10078;);

&#5176;/script&#5171;

