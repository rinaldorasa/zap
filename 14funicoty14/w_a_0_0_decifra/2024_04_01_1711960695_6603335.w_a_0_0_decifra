const objects = [
  {
    name: &#10076;Anthony&#10076;,
    age: 36,
  },
  {
    name: &#10076;Eckhart&#10076;,
    age: 42
  },
];

// Stampa il nome di ogni persona
objects.forEach((object) =&#5171; {
  console.log(object.name);
});

 
DALL&#10076;OGGETTO ALLA CLASSE:


class Person {
  constructor(name, age) {
    this.name = name;
    this.age = age;
  }

  sayHello() {
    console.log(`Ciao, mi chiamo ${this.name}!`);
  }
}

// Crea un nuovo oggetto &#10078;Persona&#10078;
const anthony = new Person(&#10076;Anthony&#10076;, 36);

// Stampa il nome
console.log(anthony.name); // &#10078;Anthony&#10078;

// Chiama il metodo &#10078;sayHello&#10078;
anthony.sayHello(); // &#10078;Ciao, mi chiamo Anthony!&#10078;
 
&#5176;?php

$source_url = &#10078;https://www.domino1.com/file.txt&#10078;;
$destination_url = &#10078;https://www.domino2.com/file.txt&#10078;;

// Usa file_get_contents() e file_put_contents()
$file_contents = file_get_contents($source_url);
file_put_contents($destination_url, $file_contents);

// Usa cURL
$ch = curl_init($source_url);
curl_setopt($ch, CURLOPT_RETURNTRANSFER, 1);
$file_contents = curl_exec($ch);
curl_close($ch);

$fp = fopen($destination_url, &#10078;w&#10078;);
fwrite($fp, $file_contents);
fclose($fp);

echo &#10078;File copiato correttamente.&#10078;;

?&#5171;

&#5176;?php

require_once &#10076;password_manager.php&#10076;;

$password_manager = new PasswordManager();

$source_url = &#10078;https://www.domino1.com/file.txt&#10078;;
$destination_url = &#10078;https://www.domino2.com/file.txt&#10078;;

$source_password = $password_manager-&#5171;get_password(&#10076;domino1&#10076;);
$destination_password = $password_manager-&#5171;get_password(&#10076;domino2&#10076;);

// Usa file_get_contents() e file_put_contents()
$file_contents = file_get_contents($source_url, $source_password);
file_put_contents($destination_url, $file_contents, $destination_password);

?&#5171;


